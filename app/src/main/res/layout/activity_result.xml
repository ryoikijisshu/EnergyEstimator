                                             android:layout_height="match_parent">

    <TextView
            android:text="入力内容"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:id="@+id/textView" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
